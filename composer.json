{
	"name": "friendsoftypo3/theme-portfolio",
	"description": "TYPO3 theme for a portfolio website",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "The TYPO3 Community",
			"role": "Contributor",
			"homepage": "https://typo3.org/community/"
		}
	],
	"require": {
		"php": ">=8.2.0",
		"typo3/cms-backend": "^13.2 || 13.*.*@dev",
		"typo3/cms-core": "^13.2 || 13.*.*@dev",
		"typo3/cms-form": "^13.2 || 13.*.*@dev",
		"typo3/cms-frontend": "^13.2 || 13.*.*@dev"
	},
	"autoload": {
		"psr-4": {
			"FriendsOfTYPO3\\ThemePortfolio\\": "Classes"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "theme_portfolio"
		}
	}
}
